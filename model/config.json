{

  //模型算法为图片分类

  "model_algorithm": "image_classification",

  // 模型类型为PyTorch

  "model_type": "PyTorch",

  // 模型推理对外api说明,包括访问协议，url，请求方法，请求接口和返回接口等

  "apis": [

    {

      "procotol": "http",

      "url": "/",

      "method": "post",

      // 请求格式为表单图片上传，key值为'images',curl 示例为 curl -X POST http://

      //{

        //{

          //endpoint

        //}

      //} -F images=@test.jpg

      "request": {

        "Content-type": "multipart/form-data",

        "data": {

          "type": "object",

          "properties": {

            "images": {

              "type": "file"

            }

          }

        }

      },

      // 返回格式为json，

      //{

      //  'logits':{

      //      'label_a': 0.1, 'label_b': -0.12, 'label_c': 0.72

      //  }

      //}

      "response": {

        "Content-type": "applicaton/json",

        "data": {

          "type": "object",

          "properties": {

            "logits": {

              "type": "object",

              "properties": {

                "label_a": {

                  "type": "number"

                },

                "label_b": {

                  "type": "number"

                },

                "label_c": {

                  "type": "number"

                }

              }

            }

          }

        }

      }

    }

  ],

  // 模型在评估集上的精度指标

  "metrics": {

    "f1": 0.102058,

    "recall": 0.9975,

    "precision": 0.05378,

    "accuracy": 1

  }

} 